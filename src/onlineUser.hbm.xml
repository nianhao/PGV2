<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.PGV2.javaBean.onlineUser" table="onlineuser">
      <meta attribute="class-description">
         This class contains the onlineUser detail. 
      </meta>
      <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="userId" column="userid" type="int"/>
      <property name="userName" column="username" type="string"/>
      <property name="showName" column="showname" type="string"/>
      <property name="loginTime" column="logintime" type="string"/>
      <property name="loginx" column="loginx" type="double"/>
      <property name="loginy" column="loginy" type="double"/>
   </class>
</hibernate-mapping>